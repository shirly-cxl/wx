/**index.wxss**/
.swiper{
 width: 750rpx;
 height: 340rpx;
}

image{
  width: 100%;
}

.cate{
  display: flex;
}

.navigator{
  flex:1;
  padding: 25rpx;
  justify-content: center;
  align-items: center;
}

.text{
  text-align: center;
}

.floor{
  overflow: hidden;
}

.title{
  width: 100%;
  font-size: 40rpx;
  font-weight: bold;
  padding: 10rpx 20rpx;
  clear: both;
}

.list-nav{
  float: left;
  width: 33.33%;
}

.list-nav:nth-last-child(-n+4){
  height: 29.6vw;
  border-left: 10rpx solid #fff;
}

.list-nav:nth-child(2),.list-nav:nth-child(3){
  border-bottom: 10rpx solid #fff;
}

.img{
  width: 100%;
  height: 100%;
}